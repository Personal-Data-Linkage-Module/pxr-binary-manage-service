{
    "name": "binary-manage-service",
    "version": "1.0.0",
    "description": "Manage binary files.",
    "license": "UNLICENSED",
    "private": true,
    "main": "index",
    "typings": "index",
    "scripts": {
        "start": "node ./dist/index.js",
        "clean": "npm-run-all -p clean:*",
        "clean:dist": "rimraf dist",
        "clean:cov": "rimraf coverage-unit",
        "build": "npm-run-all -s lint build:ts",
        "build:ts": "node ./node_modules/typescript/bin/tsc",
        "lint": "eslint ./src/index.ts src/**/*.ts",
        "lint-init": "eslint --init",
        "test:unit": "jest --coverage --forceExit --detectOpenHandles --runInBand --config=jest.unit.config.js",
        "test-jenkins:unit": "jest --coverage --reporters=jest-junit --forceExit --detectOpenHandles --runInBand --config=jest.unit.config.js",
        "jest-clear": "jest --clearCache"
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "outputDirectory": ".",
        "outputName": "xunit.xml",
        "uniqueOutputName": "false",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " > ",
        "usePathForSuiteName": "true"
    },
    "dependencies": {
