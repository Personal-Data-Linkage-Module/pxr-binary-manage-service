        "copy-to": "2.0.1",
        "cors": "2.8.5",
        "express": "4.17.1",
        "express-generator": "4.16.1",
        "express-openapi": "4.6.5",
        "express-session": "1.16.2",
        "express-winston": "3.2.1",
        "glob": "7.1.4",
        "helmet": "3.18.0",
        "http-errors": "1.6.3",
        "js-yaml": "3.13.1",
        "log4js": "6.1.2",
        "moment-timezone": "0.5.31",
        "mongodb": "3.2.7",
        "mongoose": "5.6.6",
        "node-fetch": "2.6.0",
        "node-health-service": "1.0.7",
        "node-rest-client": "3.1.0",
        "pg": "7.12.1",
        "request": "2.88.2",
        "request-context": "2.0.0",
        "sprintf-js": "1.1.2",
        "redis": "2.8.0",
        "routing-controllers": "0.7.7",
